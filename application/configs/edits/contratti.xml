<?xml version="1.0" encoding="UTF-8"?>
<edits>
    <contratti>
        <title>Contractor</title>
        <edittitle>Modifica contractor</edittitle>
        <newtitle>Nuovo contractor</newtitle>
        <savingalert>1</savingalert>
        <maintable>contratti</maintable>
        <currkey>idContratto</currkey>
        <currsql>SELECT * FROM contratti WHERE </currsql>
        <!--<mode>{{rwmode}}</mode>-->
        <form>
            <name>formContratti</name>
            <elements>
                <idContratto>
                    <type>hidden</type>
                </idContratto>
                <Contratto>
                    <type>text</type>
                    <label>Contractor</label>
                    <parameters>
                        <size>80</size>
                        <maxlen>255</maxlen>
                    </parameters>
                </Contratto>
                <Sigla>
                    <type>text</type>
                    <label>Sigla</label>
                    <parameters>
                        <size>20</size>
                        <maxlen>255</maxlen>
                    </parameters>
                </Sigla>
                <Note>
                    <label>Note</label>
                    <type>textarea</type>
                    <parameters>
                        <rows>3</rows>
                    </parameters>
                </Note> 
                <spazioverticale>
                    <type>linecustom</type>
                    <parameters>
                        <type>h3</type>
                    </parameters>
                    <description> &lt;div class="spazioverticale"&gt;&lt;/div&gt; </description> 
                </spazioverticale>
                <Conferma>
                    <type>submitreset</type>
                    <label>Conferma</label>
                </Conferma>
            </elements>
        </form>
    </contratti>
</edits>