<?xml version="1.0" encoding="UTF-8"?>
<edits>
    <schemalav>
        <title>Schema lavoro</title>
        <edittitle>Modifica schema lavoro</edittitle>
        <newtitle>Nuovo schema lavoro</newtitle>
        <savingalert>1</savingalert>
        <maintable>tab_schemi_lav</maintable>
        <currkey>idTabSchemiLav</currkey>
        <currsql>SELECT * FROM tab_schemi_lav WHERE </currsql>
        <!--<mode>{{rwmode}}</mode>-->
        <form>
            <name>formschemalav</name>
            <elements>
                <idTabSchemiLav>
                    <type>hidden</type>
                </idTabSchemiLav>
                <descSchemaLav>
                    <type>text</type>
                    <label>Descrizione</label>
                    <parameters>
                        <size>80</size>
                        <maxlen>255</maxlen>
                    </parameters>
                </descSchemaLav>
                <oreSett>
                    <type>text</type>
                    <label>Ore lavorate per settimana</label>
                    <parameters>
                        <size>4</size>
                        <maxlen>10</maxlen>
                        <Locale>it-IT</Locale>
                    </parameters>
                </oreSett>
                <oreMese>
                    <type>text</type>
                    <label>Ore lavorate per mese</label>
                    <parameters>
                        <size>4</size>
                        <maxlen>10</maxlen>
                        <Locale>it-IT</Locale>
                    </parameters>
                </oreMese>
                <ggSett>
                    <type>text</type>
                    <label>Giorni lavorati per settimana</label>
                    <parameters>
                        <size>4</size>
                        <maxlen>10</maxlen>
                        <Locale>it-IT</Locale>
                    </parameters>
                </ggSett>
                <ggMese>
                    <type>text</type>
                    <label>Giorni lavorati per mese</label>
                    <parameters>
                        <size>4</size>
                        <maxlen>10</maxlen>
                        <Locale>it-IT</Locale>
                    </parameters>
                </ggMese>
                <spazioverticale>
                    <type>linecustom</type>
                    <parameters>
                        <type>h3</type>
                    </parameters>
                    <description> &lt;div class="spazioverticale"&gt;&lt;/div&gt; </description> 
                </spazioverticale>
                <Conferma>
                    <type>submitreset</type>
                    <label>Conferma</label>
                </Conferma>
            </elements>
        </form>
    </schemalav>
</edits>