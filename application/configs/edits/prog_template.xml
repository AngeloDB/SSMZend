<?xml version="1.0" encoding="UTF-8"?>
<edits>
    <prog_template>
        <title>Modello progetto</title>
        <edittitle>Modifica modello progetto</edittitle>
        <newtitle>Nuovo modello progetto</newtitle>
        <savingalert>1</savingalert>
        <maintable>prog_template</maintable>
        <currkey>idProgTemplate</currkey>
        <currsql>SELECT * FROM prog_template WHERE </currsql>
        <!--<mode>{{rwmode}}</mode>-->
        <form>
            <name>formProgTemplate</name>
            <elements>
                <idProgTemplate>
                    <type>hidden</type>
                </idProgTemplate>
                <Nome>
                    <type>text</type>
                    <notempty>true</notempty>
                    <label>Modello progetto</label>
                    <parameters>
                        <size>80</size>
                        <maxlen>255</maxlen>
                    </parameters>
                </Nome>
                <Note>
                    <label>Note</label>
                    <type>textarea</type>
                    <parameters>
                        <rows>3</rows>
                    </parameters>
                </Note> 
                <spazioverticale>
                    <type>linecustom</type>
                    <parameters>
                        <type>h3</type>
                    </parameters>
                    <description> &lt;div class="spazioverticale"&gt;&lt;/div&gt; </description> 
                </spazioverticale>
                <Conferma>
                    <type>submitreset</type>
                    <label>Conferma</label>
                </Conferma>
            </elements>
        </form>
    </prog_template>
</edits>