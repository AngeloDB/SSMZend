<grids>
    <anomalie>
        <title>Osservazioni</title>
        <addlink>/crud/new/edt/anomalie</addlink>
        <addtitle>Nuova osservazione</addtitle>
        <select>
            <from>
                <aliasname>
                    <A>anomalie</A>
                </aliasname>
                <fields>
                    <idAnomalia>idAnomalia</idAnomalia>
                    <idSopralluogo>idSopralluogo</idSopralluogo>
                    <idCantiere>idCantiere</idCantiere>
                    <idImpresa>idImpresa</idImpresa>
                    <Descrizione>Descrizione</Descrizione>
                    <Azione>Azione</Azione>
                    <Scadenza>Scadenza</Scadenza>
                </fields>
            </from>
            <join id='cantieri'>
                <cond>B.idCantiere = A.idCantiere</cond>
                <aliasname>
                    <B>cantieri</B>
                </aliasname>
                <fields>
                    <Cantiere>Cantiere</Cantiere>
                </fields>
            </join>
            <join id='imprese'>
                <cond>C.idImpresa = A.idImpresa</cond>
                <aliasname>
                    <C>imprese</C>
                </aliasname>
                <fields>
                    <Impresa>Impresa</Impresa>
                </fields>
            </join>
            <join id='sopralluoghi'>
                <cond>D.idSopralluogo = A.idSopralluogo</cond>
                <aliasname>
                    <D>sopralluoghi</D>
                </aliasname>
                <fields>
                    <Data>Data</Data>
                </fields>
            </join>
        </select>
        <updatecolumns>
            <idAnomalia>
                <hidden>1</hidden>
            </idAnomalia>
            <idSopralluogo>
                <hidden>1</hidden>
            </idSopralluogo>
            <idImpresa>
                <hidden>1</hidden>
            </idImpresa>
            <idCantiere>
                <hidden>1</hidden>
            </idCantiere>
            <Data>
                <title>Data</title>
                <format>date</format>
                <format>
                    <locale>it-IT</locale>
                    <date_format>dd/MM/y</date_format>
                </format>
            </Data>
            <Scadenza>
                <title>Scadenza</title>
                <format>date</format>
                <format>
                    <locale>it-IT</locale>
                    <date_format>dd/MM/y</date_format>
                </format>
            </Scadenza>
        </updatecolumns>
        <extracolumns>
            <Edit>
                <position>left</position>
                <title>Edit</title>
                <decorator>
                    /crud/edit/id/{{idAnomalia}}/edt/anomalie#/images/edit.png#Edit
                </decorator>
            </Edit>
        </extracolumns>
    </anomalie>
